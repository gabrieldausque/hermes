{
  "name": "Hermes",
  "description": "POC application for Hermes POC",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "gabriel",
    "email": "gabrieldausque.pro@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^12.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run compile && npm run mocha",
    "dev": "npm run compile && ts-node-dev --no-notify src/",
    "start": "npm run compile && node lib",
    "mocha": "ts-mocha \"test/**/*.ts\" --recursive --exit",
    "compile": "shx rm -rf lib/ && tsc && node_modules/.bin/ncp src/services/topic/browser/ public/lib/topic/",
    "docs": "npm run compile && ./node_modules/.bin/typedoc --options typedoc-html.json src/services/topic && ./node_modules/.bin/typedoc --options typedoc-md.json src/services/topic",
    "build": "shx rm -rf src/services/topic/lib/ && tsc -p src/services/topic && node_modules/.bin/ncp src/services/topic/browser/ src/services/topic/lib/browser",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/configuration": "^4.5.2",
    "@feathersjs/errors": "^4.5.2",
    "@feathersjs/express": "^4.5.2",
    "@feathersjs/feathers": "^4.5.2",
    "@feathersjs/socketio": "^4.5.2",
    "@feathersjs/socketio-client": "^4.5.2",
    "@feathersjs/transport-commons": "^4.5.2",
    "@types/node": "^13.11.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-memory": "^4.1.0",
    "feathers-swagger": "^1.1.1",
    "guid-typescript": "^1.0.9",
    "helmet": "^3.22.0",
    "jsdom": "^16.2.1",
    "serve-favicon": "^2.5.0",
    "swagger-ui": "^3.25.0",
    "swagger-ui-dist": "^3.25.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^7.0.2",
    "uuidv4": "^6.0.7",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/helmet": "0.0.45",
    "@types/mocha": "^7.0.2",
    "@types/serve-favicon": "^2.5.0",
    "axios": "^0.19.2",
    "mocha": "^7.1.1",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.2",
    "shx": "^0.3.2",
    "ts-mocha": "^7.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.17.4",
    "typedoc-plugin-markdown": "^2.2.17",
    "typedoc-plugin-markdown-pages": "^0.3.0",
    "typescript": "^3.8.3"
  }
}
